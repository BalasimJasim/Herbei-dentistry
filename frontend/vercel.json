{
  "version": 2,
  "routes": [
    {
      "src": "^/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/assets/$1"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/(.*).png",
      "dest": "/$1.png"
    },
    {
      "src": "^/patient-portal",
      "dest": "/index.html"
    },
    {
      "src": "^/dashboard",
      "dest": "/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/.*",
      "dest": "/index.html"
    }
  ],
  "framework": "vite"
} 